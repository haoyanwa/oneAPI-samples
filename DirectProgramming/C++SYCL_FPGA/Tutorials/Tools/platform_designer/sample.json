{
  "guid": "FC8F050C-49F1-45D9-81F0-DB55E81E592A",
  "name": "Platform Designer - Intel® Cyclone® V SoC",
  "categories": ["Toolkit/oneAPI Direct Programming/C++SYCL FPGA/Tutorials/Tools"],
  "description": "An Intel® FPGA tutorial demonstrating how to export a reusable IP component to Intel® Quartus® Prime Standard and Platform Designer.",
  "toolchain": ["icpx"],
  "os": ["linux", "windows"],
  "targetDevice": ["FPGA"],
  "builder": ["cmake"],
  "languages": [{"cpp":{}}],
  "commonFolder": {
    "base": "../../../",
    "include": [
      "README.md",
      "Tutorials/Tools/platform_designer_standard",
      "include"
    ],
    "exclude": []
  },
  "ciTests": {
    "linux": [
      {
        "id": "fpga_emu",
        "steps": [
          "icpx --version",
          "mkdir add_oneapi/build",
          "cd add_oneapi/build",
          "cmake ..",
          "make fpga_emu",
          "./add.fpga_emu"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icpx --version",
          "mkdir add_oneapi/build",
          "cd add_oneapi/build",
          "cmake ..",
          "make report"
        ]
      }
    ],
    "windows": [
      {
        "id": "fpga_emu",
        "steps": [
          "icx-cl --version",
          "cd ../../..",
          "mkdir build",
          "cd build",
          "cmake -G \"NMake Makefiles\" ../Tutorials/Tools/platform_designer_standard/add_oneapi",
          "nmake fpga_emu",
          "add.fpga_emu.exe"
        ]
      },
      {
        "id": "report",
        "steps": [
          "icx-cl --version",
          "cd ../../..",
          "mkdir build",
          "cd build",
          "cmake -G \"NMake Makefiles\" ../Tutorials/Tools/platform_designer_standard/add_oneapi",
          "nmake report"
        ]
      }
    ]
  },
  "expertise": "Getting Started"
}
